local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("nihub", "DarkTheme")

local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")

    MainSection:NewButton("Daptoper Game", "Daptoper!", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/ni7kobv/nihub/main/scripts/Daptoper%20Game"))()
    end)

    MainSection:NewButton("Every Second You Get +1 Jump Power", "Every", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/ni7kobv/nihub/main/scripts/Every%20Second%20You%20Get%20%2B1%20Jump%20Powere"))()
    end)

    MainSection:NewButton("Lucky Block", "Block", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/ni7kobv/nihub/main/scripts/lucky%20block"))()
    end)
