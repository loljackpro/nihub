local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("nihub v1.0.1", "DarkTheme")

local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")

getgenv().autofarm = true



MainSection:NewToggle("Auto Farm", "Farm", function(state)
    if state then
 
       while wait() do
if not getgenv().autofarm then break end
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-54.6598244, 67.4162369, -342.489105, 0.999963462, -2.21376606e-09, -0.00854752865, 1.91392457e-09, 1, -3.5087492e-08, 0.00854752865, 3.50698528e-08, 0.999963462)


local teleport_table = {
    location1 = Vector3.new(-55.6173477, 108.030296, 9497.65332, -0.999553442, 3.08223536e-09, 0.0298809353, 3.49372685e-13, 1, -1.03138873e-07, -0.0298809353, -1.03092809e-07, -0.999553442)
}

local tween_s = game:GetService('TweenService')
local tweeninfo = TweenInfo.new(20,Enum.EasingStyle.Linear)

local lp = game.Players.LocalPlayer

function bypass_teleport(v)
    if lp.Character and 
    lp.Character:FindFirstChild('HumanoidRootPart') then
        local cf = CFrame.new(v)
        local a = tween_s:Create(lp.Character.HumanoidRootPart,tweeninfo,{CFrame=cf})
        a:Play()
        -- a.Completed:Wait()
        -- print('Teleporting Done!')
    end
end

bypass_teleport(teleport_table.location1)
wait(39)
end

    else
        getgenv().autofarm = false
    end
end)

    MainSection:NewButton("ctrl+click tp", "teleport for you", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/ni7kobv/nihub/main/scripts/ctrl%20click%20tp"))()
    end)

    local Other = Window:NewTab("Teleport")
    local OtherSection = Other:NewSection("Teleport")

    OtherSection:NewButton("Teleport to white team", "Teleport", function()
               game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-19.4842529, -13.100008, -592.937622, 0, 0, -1, 0, 1, 0, 1, 0, 0)
    end)

    OtherSection:NewButton("Teleport to red team", "Teleport", function()
               game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(469.263947, -13.100008, -34.4133759, -1, 0, 0, 0, 1, 0, 0, 0, -1)
    end)

    OtherSection:NewButton("Teleport to dark team", "Teleport", function()
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-576.394958, -13.1000061, -99.8001022, 1, 0, 0, 0, 1, 0, 0, 0, 1)
    end)

    OtherSection:NewButton("Teleport to blue team", "Teleport", function()
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(469.263947, -13.100008, 330.586609, -1, 0, 0, 0, 1, 0, 0, 0, -1)
    end)

    OtherSection:NewButton("Teleport to green team", "Teleport", function()
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-576.394958, -13.1000061, 263.19989, 1, 0, 0, 0, 1, 0, 0, 0, 1)
    end)

    OtherSection:NewButton("Teleport to purple team", "Teleport", function()
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(469.263947, -13.100008, 677.586609, -1, 0, 0, 0, 1, 0, 0, 0, -1)
    end)

    OtherSection:NewButton("Teleport to yellow team", "Teleport", function()
               game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-576.394958, -13.1000061, 610.19989, 1, 0, 0, 0, 1, 0, 0, 0, 1)
    end)
    

    local Other = Window:NewTab("Misc")
    local OtherSection = Other:NewSection("Misc")

OtherSection:NewKeybind("Keybind", "Keybind", Enum.KeyCode.RightControl, function()
	Library:ToggleUI()
end)

    OtherSection:NewButton("Rejoin", "rejoin for you", function()
local teleportservice = game:GetService("TeleportService")
teleportservice:Teleport(game.PlaceId)
    end)

    OtherSection:NewButton("discord link", "copy discord link", function()
               game.StarterGui:SetCore("SendNotification", {
        Title = 'Discord';
        Text = 'discord link copied in your keyboard!';
        Duration = 1;
       })
       setclipboard('https://discord.gg/ah7nGsWCbU')
    end)
    
    game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Open/Close", Text = "RightControl to Open/Close"})





